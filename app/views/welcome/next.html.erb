<div class="jumbotron">
  <h1>SquidList</h1>
  <p>Your Personal Surf and Music Calendar</p>
  <div class= "pull-right", style= "margin-top: -184px;">
    <img src='assets/squid_list.jpg' %>
  </div>  
</div>
<div class="container">
  <div class="row">
    <div class="md-4">
      <h2>Your SquidList events this month</h2>
      <div class="container marketing">
        <div class="row"> 
          <% @all_events.each do |e| %>
            <%= render e %>
          <% end %>
        </div>
      </div> 
    </div>
  </div>
</div>  