  <div class="jumbotron">
    <h1>SquidList</h1>
    <p>Your Surf and Music Calendar</p>
  </div>
  <div class="container">
    <% if @all_events.empty? && current_user %>
      <%= link_to "Set you Interests", new_user_interest_path %>
      <%# Display an awesome default image of what users wil see when they join, underneath #%>
    <% elsif @all_events.empty? %>
      <%= link_to "Sign up", new_user_registration_path %> 
    <% else %>   
      <div class="row">
        <div class="md-4">
          <h2>Your SquidList events this week</h2>
          <div class="container marketing">
            <div class="row"> 
              <% @all_events.each do |e| %>
                <%= render e %>
              <% end %>
            </div>
          </div> 
        </div>
      </div>
    
    <% end %>
  </div>  



  
