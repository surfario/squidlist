<div class="container marketing">
  <div class="row"> 
      <% @forecasts.each do |f| %>
        <% if ["3", "4", "5"].include?(f.quality) %>
          <div class="col-lg-4"> 
            <img class="img-circle" src="data:image/gif;base64,R0lGODlhAQABAIAAAHd3dwAAACH5BAAAAAAALAAAAAABAAEAAAICRAEAOw==" alt="Generic placeholder image" style="width: 140px; height: 140px;">
            <p><%= link_to f.surf_break.name, f.surf_break %></p>
            <p><%= f.date.strftime("%B %d, %Y") %></p>
            <p>Quality: <%= f.quality %></p>
            <p>Height: <%= f.height %></p>
            </p>
          </div>
        <% end %>  
      <% end %>    
  </div>
</div>    